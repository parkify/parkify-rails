= form_for @quick_property do |f|
  -if @quick_property.errors.any?
    #error_explanation
      %h2= "#{pluralize(@quick_property.errors.count, "error")} prohibited this quick_property from being saved:"
      %ul
        - @quick_property.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :key
    = f.text_field :key
  .field
    = f.label :value
    = f.text_field :value
  .actions
    = f.submit 'Save'
